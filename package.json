{
  "name": "budget-application",
  "version": "1.0.0",
  "description": "Project 2",
  "main": "server.js",
  "scripts": {
    "start": "nodemon server.js",
    "lint": "eslint **/*.js --quiet",
    "fix": "eslint --fix .",
    "test": "npm run lint && NODE_ENV=test mocha -u tdd --reporter spec --exit",
    "css-build": "node-sass --omit-source-map-url public/styles/styles.scss public/styles/sassStyles.css",
    "css-watch": "npm run css-build -- --watch",
    "css-start": "npm run css-watch",
    "start-dev": "concurrently \"npm start\" \"npm run css-start\""
  },
  "license": "ISC",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "bulma": "^0.7.1",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.15.6",
    "mysql2": "^1.6.1",
    "node-sass": "^4.9.3",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "sequelize": "^4.38.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "concurrently": "^3.6.1",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.2",
    "mocha": "^5.2.0",
    "prettier": "^1.13.7"
  }
}
